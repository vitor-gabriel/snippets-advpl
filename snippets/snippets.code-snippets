    "tReport1": {
        "prefix": "tReport 1 Sessão",
        "body": [
            "/*//#########################################################################################",
            "Projeto : ${project}",
            "Modulo  : ${module}",
            "Fonte   : ${TM_FILENAME_BASE}",
            "Objetivo: ${objetivo}",
            "*///#########################################################################################",
            "",
            "#INCLUDE 'TOTVS.CH'",
            "#INCLUDE 'REPORT.CH'",
            "",
            "/*/{Protheus.doc} ${TM_FILENAME_BASE}",
            "\tGerenciador de Processamento",
            "",
            "\t@author  ${Nome}",
            "\t@example ${Exemplos}",
            "\t@param   [${Nome_do_Parametro}],${Tipo_do_Parametro},${Descricao_do_Parametro}",
            "\t@return  ${Especifica_o_retorno}",
            "\t@table   ${Tabelas}",
            "\t@since   ${CURRENT_DATE}-${CURRENT_MONTH}-${CURRENT_YEAR}",
            "/*/",
            "User Function ${TM_FILENAME_BASE}()",
            "\tLocal cPergRel := PadR('${TM_FILENAME_BASE}',10)",
            "\tLocal oReport",
            "\tlocal ccAlias := getNextAlias()",
            "",
            "\tPergunte(cPergRel,.T.)",
            "",
            "\toReport := xPrintRel(ccAlias,cPergRel)",
            "\toReport:PrintDialog()",
            "",
            "Return",
            "",
            "/*/{Protheus.doc} xPrintRel",
            "\tImprimir Relatório",
            "",
            "\t@author  ${Nome}",
            "\t@example ${Exemplos}",
            "\t@param   [${Nome_do_Parametro}],${Tipo_do_Parametro},${Descricao_do_Parametro}",
            "\t@return  ${Especifica_o_retorno}",
            "\t@table   ${Tabelas}",
            "\t@since   ${CURRENT_DATE}-${CURRENT_MONTH}-${CURRENT_YEAR}",
            "/*/",
            "Static Function xPrintRel(ccAlias,cPerg)",
            "",
            "\tlocal oReport",
            "\tlocal oSection1",
            "",
            "\toReport   := TReport():New(cPerg,'${title}',cPerg,{|oReport|ReportPrint(oReport,ccAlias)},'')",
            "\toReport:SetLandScape()",
            "\toReport:SetTotalInLine(.F.)",
            "",
            "\toSection1 := TRSection():New(oReport)",
            "\toSection1:SetTotalInLine(.F.)",
            "",
            "Return oReport",
            "",
            "",
            "/*/{Protheus.doc} ReportPrint",
            "\tMontar query e abrir em tabela temporária",
            "",
            "\t@author  ${Nome}",
            "\t@example ${Exemplos}",
            "\t@param   [${Nome_do_Parametro}],${Tipo_do_Parametro},${Descricao_do_Parametro}",
            "\t@return  ${Especifica_o_retorno}",
            "\t@table   ${Tabelas}",
            "\t@since   ${CURRENT_DATE}-${CURRENT_MONTH}-${CURRENT_YEAR}",
            "/*/",
            "static function ReportPrint(oReport, ccAlias,cOrdem)",
            "\tlocal oSection1b := oReport:Section(1)",
            "",
            "\toSection1b:BeginQuery()",
            "\tBeginSQL Alias ccAlias",
            "\t    %noparser%",
            "",
            "\t    ${query}",
            "",
            "\tEndSql",
            "\toSection1b:EndQuery()",
            "",
            "\toReport:SetMeter((ccAlias)->(RecCount()))",
            "",
            "\toSection1b:Print()",
            "Return"
        ],
        "description": "Monta um modelo de tReport com 1 Sessão."
    },
