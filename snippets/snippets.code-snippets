            "\t",
            "\toBreak1 := TRBreak():New(oSection1,{|| oSection4:Cell('${cellSession1}') },'Total:',.F.)",
            "\tTRFunction():New(oSection4:Cell('${cellSession4}' ), 'TOT1', 'SUM', oBreak1,,,, .F., .T.)",
            "",
            "\toBreak2 := TRBreak():New(oSection2,{|| oSection4:Cell('${cellSession2}') },'Total:',.F.)",
            "\tTRFunction():New(oSection4:Cell('${cellSession4}' ), 'TOT11', 'SUM', oBreak2,,,, .F., .T.)",
            "",
            "\toBreak3 := TRBreak():New(oSection3,{|| oSection4:Cell('${cellSession3}') },'Total:',.F.)",
            "\tTRFunction():New(oSection4:Cell('${cellSession4}' ), 'TOT111', 'SUM', oBreak3,,,, .F., .T.)",
            "Return oReport",
            "",
